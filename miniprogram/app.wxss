/**app.wxss**/
.container {
    min-height: 100%;
    display: flex;
    flex-direction: column;
    background-color: #f5f5f5;
    font-size: 16px;
    color: #333;
}

button {
    background-color: #07c160;
    color: white;
    border-radius: 8px;
    font-size: 16px;
    padding: 8px 16px;
}

button.reject-btn {
    background-color: #f43f30;
}


page {
    background: #f6f6f6;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}